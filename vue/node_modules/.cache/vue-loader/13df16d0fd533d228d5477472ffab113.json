CAAC,CAAC;IACT,CAAC,CAAC,CAAC,CAAC,CAAC","file":"D:/IdeaProjects/springboot-vue-demo/vue/src/layout/Layout.vue","sourceRoot":"","sourcesContent":["<template>\n    <div>\n        <!--    头部-->\n        <Header :user=\"user\"/>\n\n        <!--    主体-->\n        <div style=\"display: flex\">\n            <!--      侧边栏-->\n            <Aside />\n            <!--      内容区域-->\n            <router-view style=\"flex: 1\" @userInfo=\"refreshUser\"/>\n        </div>\n    </div>\n</template>\n\n<script>\n    import Header from \"@/components/Header\";\n    import Aside from \"@/components/Aside\";\n    import request from \"@/utils/request\";\n\n    export default {\n        name: \"Layout\",\n        components: {\n            Header,\n            Aside\n        },\n        data() {\n            return {\n                user: {}\n            }\n        },\n        created() {\n            this.refreshUser()\n        },\n        methods: {\n            refreshUser() {\n                let userJson = sessionStorage.getItem(\"user\");\n                if (!userJson) {\n                    return\n                }\n                let userId = JSON.parse(userJson).id\n                request.get(\"/user/get/\" + userId).then(res => {\n                    this.user = res.data.data\n                })\n            }\n        }\n    }\n</script>"]}]}