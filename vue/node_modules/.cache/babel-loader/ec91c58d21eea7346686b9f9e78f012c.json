;;AAAA,WAAzB,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACpB;AAAA,qBAAgE,CAAhE,YAAA,CAAgE,mBAAhE,EAAgE;4BAA7C,KAAA,CAAA,IAAA,CAAK,QAAwC;;yBAA7C,KAAA,CAAA,IAAA,CAAK,QAAL,GAAa,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAAhE,E,IAAA,E,CAAA,E,cAAA,CAAgE,CAAhE;AAAA,a,CADoB;;AAAA,WAAxB,CADe,EAIf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACpB;AAAA,qBAA2D,CAA3D,YAAA,CAA2D,mBAA3D,EAA2D;4BAAxC,KAAA,CAAA,IAAA,CAAK,GAAmC;;yBAAxC,KAAA,CAAA,IAAA,CAAK,GAAL,GAAQ,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA3D,E,IAAA,E,CAAA,E,cAAA,CAA2D,CAA3D;AAAA,a,CADoB;;AAAA,WAAxB,CAJe,EAOf,YAAA,CAIe,uBAJf,EAIe;AAJD,YAAA,KAAK,EAAC;AAIL,WAJf,EAAwB;8BACpB;AAAA,qBAAmD,CAAnD,YAAA,CAAmD,mBAAnD,EAAmD;4BAAhC,KAAA,CAAA,IAAA,CAAK,GAA2B;;yBAAhC,KAAA,CAAA,IAAA,CAAK,GAAL,GAAQ,M;kBAAwB;AAAtB,gBAAA,KAAK,EAAC;AAAgB,eAAnD,E;kCAAuC;AAAA,yBAAC,C,WAAA,CAAD;AAAA,iB;;eAAvC,E,CAAA,E,cAAA,CAAmD,EACnD,YAAA,CAAmD,mBAAnD,EAAmD;4BAAhC,KAAA,CAAA,IAAA,CAAK,GAA2B;;yBAAhC,KAAA,CAAA,IAAA,CAAK,GAAL,GAAQ,M;kBAAwB;AAAtB,gBAAA,KAAK,EAAC;AAAgB,eAAnD,E;kCAAuC;AAAA,yBAAC,C,WAAA,CAAD;AAAA,iB;;eAAvC,E,CAAA,E,cAAA,CADmD,EAEnD,YAAA,CAAqD,mBAArD,EAAqD;4BAAlC,KAAA,CAAA,IAAA,CAAK,GAA6B;;yBAAlC,KAAA,CAAA,IAAA,CAAK,GAAL,GAAQ,M;kBAA0B;AAAxB,gBAAA,KAAK,EAAC;AAAkB,eAArD,E;kCAAwC;AAAA,yBAAE,C,WAAA,CAAF;AAAA,iB;;eAAxC,E,CAAA,E,cAAA,CAFmD,CAAnD;AAAA,a,CADoB;;AAAA,WAAxB,CAPe,EAYf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACpB;AAAA,qBAA+E,CAA/E,YAAA,CAA+E,mBAA/E,EAA+E;AAArE,gBAAA,IAAI,EAAC,UAAgE;4BAA5C,KAAA,CAAA,IAAA,CAAK,OAAuC;;yBAA5C,KAAA,CAAA,IAAA,CAAK,OAAL,GAAY,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA/E,E,IAAA,E,CAAA,E,cAAA,CAA+E,CAA/E;AAAA,a,CADoB;;AAAA,WAAxB,CAZe,CAFf;AAAA,S;;OADJ,E,CAAA,E,SAAA,CAkBU,CAlBV;AAAA,K;;GADJ,E,CAAA,E,cAAA,CA6BE,CAzCN,CA0CI,CAlGN,C","sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n<!--    功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n    </div>\n<!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"请输入关键字\" style=\"width: 20%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n<!--      表格区域-->\n    <el-table\n            stripe\n            :data=\"tableData\"\n            style=\"width: 100%\">\n      <el-table-column\n              prop=\"id\"\n              label=\"ID\"\n              sortable\n      >\n      </el-table-column>\n      <el-table-column\n              prop=\"username\"\n              label=\"用户名\">\n      </el-table-column>\n      <el-table-column\n              prop=\"nickName\"\n              label=\"昵称\">\n      </el-table-column>\n        <el-table-column\n                prop=\"age\"\n                label=\"年龄\">\n        </el-table-column>\n        <el-table-column\n                prop=\"sex\"\n                label=\"性别\">\n        </el-table-column>\n        <el-table-column\n                prop=\"address\"\n                label=\"地址\">\n        </el-table-column>\n      <el-table-column label=\"操作\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n<!--确认删除气泡框-->\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n<!--      分页区域-->\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\"\n              :current-page=\"currentPage\"\n              :page-sizes=\"[5, 10, 20]\"\n              :page-size=\"pageSize\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              :total=\"total\">\n      </el-pagination>\n\n\n        <el-dialog title=\"提示\" v-model=\"dialogVisible\" width=\"30%\">\n            <el-form :model=\"form\" label-width=\"120px\">\n                <el-form-item label=\"用户名\">\n                    <el-input v-model=\"form.username\" style=\"width: 80%\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"昵称\">\n                    <el-input v-model=\"form.nickName\" style=\"width: 80%\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"年龄\">\n                    <el-input v-model=\"form.age\" style=\"width: 80%\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"性别\">\n                    <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\n                    <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\n                    <el-radio v-model=\"form.sex\" label=\"未知\">未知</el-radio>\n                </el-form-item>\n                <el-form-item label=\"地址\">\n                    <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%\"></el-input>\n                </el-form-item>\n            </el-form>\n            <template #footer>\n                <span class=\"dialog-footer\">\n                  <el-button @click=\"dialogVisible = false\">取 消</el-button>\n                  <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n                </span>\n            </template>\n        </el-dialog>\n\n\n    </div>\n  </div>\n</template>\n\n<script>\n\n\n\nimport request from \"../utils/request\";\n\n\nexport default {\n    name: 'Home',\n    components: {\n    },\n\n    data(){\n        return{\n            form: {},\n            dialogVisible: false,\n            search: '',\n            currentPage:1,\n            total:0,\n            pageSize:10,\n            // total可以从后台获取\n            tableData:[]\n        }\n    },\n    created() {\n        this.load()\n    },\n    methods: {\n        load() {\n            request.get(\"/user/get\", {\n                params: {\n                    pageNum: this.currentPage,\n                    pageSize: this.pageSize,\n                    search: this.search\n                }\n            }).then(res => {\n                console.log(res);\n                this.tableData = res.data.data.records\n                this.total = res.data.data.total\n            })\n        },\n        add() {\n            this.dialogVisible = true;\n            this.form = {}\n        },\n        save() {\n            if (this.form.id) {    //update\n                request.put(\"/user/update\", this.form).then(res => {\n                    console.log(res)\n                    if (res.data.code === '0') {\n                        this.$message({\n                            type: \"success\",\n                            message: \"更新成功\"\n                        })\n                    } else {\n                        this.$message({\n                            type: \"error\",\n                            message: res.data.msg\n                        })\n                    }\n\n                })\n            } else {       //new\n                request.post(\"/user\", this.form).then(res => {\n                    console.log(res)\n                    if (res.data.code === '0') {\n                        this.$message({\n                            type: \"success\",\n                            message: \"新增成功\"\n                        })\n                    } else {\n                        this.$message({\n                            type: \"error\",\n                            message: res.data.msg\n                        })\n                    }\n\n                })\n            }\n            this.load();   //刷新表格数据\n\n            this.dialogVisible = false;\n\n        },\n        handleEdit(row) {\n            this.form = JSON.parse(JSON.stringify(row))\n            // 避免修改后点取消表格发生变化，故要深拷贝\n            this.dialogVisible = true\n\n        },\n        handleSizeChange(pageSize) {\n            this.pageSize=pageSize\n            this.load()\n        },\n        handleCurrentChange(pageNum) {\n            this.currentPage=pageNum\n            this.load()\n        },\n        handleDelete(id){\n            console.log(id)\n            request.delete(\"user/delete/\" + id).then(res=>{\n                if (res.data.code === '0') {\n                    this.$message({\n                        type: \"success\",\n                        message: \"删除成功\"\n                    })\n                } else {\n                    this.$message({\n                        type: \"error\",\n                        message: res.data.msg\n                    })\n                }\n                this.load() //reload the table\n            })\n        }\n    }\n}\n</script>\n"],"sourceRoot":""}]}